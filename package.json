{
  "name": "vue-ts-boilerplate",
  "version": "0.0.0",
  "private": true,
  "author": "amurtuzov <amurtuzov92@gmail.com>",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.vue, --ignore-path .gitignore --fix src",
    "kill": "lsof -t -i tcp:3000,8080 | xargs kill -9",
    "test": "vitest run",
    "version-update": "yarn config set version-git-tag false && yarn version --new-version $VERSION && git push --tags &&  git add . && git commit -m \"version update\" && git push && echo \"Successfully released version: $VERSION\"",
    "post-release": "git checkout master && git push && git push --tags && git checkout develop && git push"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "lodash": "^4.17.21",
    "pinia": "^2.1.6",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.43.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@types/lodash": "^4.14.198",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "lint-staged": "^14.0.1",
    "msw": "^1.3.1",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "terser": "^5.19.4",
    "typescript": "~5.2.2",
    "vite": "^5.0.7",
    "vite-plugin-eslint": "^1.8.1",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.11",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "yarn lint"
    ]
  }
}
